      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.splitsmart.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6e5cb04b-711b-4fc6-8502-887703e25b3b"
      }
    },
    "owner": "sattamt"
  }
